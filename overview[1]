        <!-- Marketo Munchkin: Start -->
        <script type="text/javascript">
            (function () {
                var didInit = false;
                function initMunchkin() {
                    if (didInit === false) {
                        didInit = true;
                        Munchkin.init('387-XNW-688');
                    }
                }
                var s = document.createElement('script');
                s.type = 'text/javascript';
                s.async = true;
                s.src = '//munchkin.marketo.net/munchkin.js';
                s.onreadystatechange = function () {
                    if (this.readyState == 'complete' || this.readyState == 'loaded') {
                        initMunchkin();
                    }
                };
                s.onload = initMunchkin;

                // this portion of the code was added and not part of the original
                // snippet from marketo we are doing some weird redirection stuff
                // and is causing some issues
                const head = document.getElementsByTagName('head');

                document.addEventListener('DOMContentLoaded', function () {
                    // doing the append here to make sure that head is available
                    head[0].appendChild(s);
                });

                if (!head?.length) {
                    console.warn(
                        'No head tag found in the document when loading Marketo Munchkin script',
                    );
                }
            })();
        </script>
